{
  "name": "student",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "start:test": "NODE_ENV=test expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:unit": "jest --testMatch='**/*.unit.test.ts'",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:deep-linking": "playwright test deepLinking.e2e.test.ts",
    "test:accessibility": "playwright test --grep='accessibility'",
    "test:cultural": "playwright test --grep='cultural'",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "expo": "~53.0.20",
    "expo-linking": "~6.3.1",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-dom": "^18.3.1",
    "react-native": "0.76.2",
    "react-native-gesture-handler": "~2.18.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "3.34.0",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.2.79",
    "typescript": "~5.3.3"
  },
  "private": true
}
