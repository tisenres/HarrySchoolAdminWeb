# Harry School Student App Environment Variables
# Copy this file to .env and fill in your actual values

# Environment
EXPO_PUBLIC_ENV=development

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://xlcsegukheumsadygmgh.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# OpenAI Configuration
EXPO_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here

# Firebase Configuration  
EXPO_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
EXPO_PUBLIC_FIREBASE_PROJECT_ID=harry-school-firebase-project
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id_here
EXPO_PUBLIC_FIREBASE_APP_ID=your_firebase_app_id_here

# App Configuration
EXPO_PUBLIC_APP_NAME=Harry School Student
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_APP_BUNDLE_ID=uz.harryschool.student

# Islamic Features Configuration
EXPO_PUBLIC_ISLAMIC_FEATURES_ENABLED=true
EXPO_PUBLIC_PRAYER_TIMES_ENABLED=true
EXPO_PUBLIC_HIJRI_CALENDAR_ENABLED=true
EXPO_PUBLIC_RAMADAN_MODE_ENABLED=true

# Cultural Settings
EXPO_PUBLIC_DEFAULT_LOCALE=uz
EXPO_PUBLIC_SUPPORTED_LOCALES=en,ru,uz,ar
EXPO_PUBLIC_RTL_SUPPORT_ENABLED=true
EXPO_PUBLIC_ARABIC_FONTS_ENABLED=true

# Educational Compliance
EXPO_PUBLIC_COPPA_COMPLIANCE_ENABLED=true
EXPO_PUBLIC_FERPA_COMPLIANCE_ENABLED=true
EXPO_PUBLIC_MINIMUM_AGE=13
EXPO_PUBLIC_PARENTAL_CONSENT_REQUIRED=true

# Security Settings
EXPO_PUBLIC_CERTIFICATE_PINNING_ENABLED=true
EXPO_PUBLIC_ROOT_DETECTION_ENABLED=true
EXPO_PUBLIC_DEBUG_MODE_ENABLED=false

# Analytics and Monitoring
EXPO_PUBLIC_ANALYTICS_ENABLED=true
EXPO_PUBLIC_CRASH_REPORTING_ENABLED=true
EXPO_PUBLIC_PERFORMANCE_MONITORING_ENABLED=true