# Harry School Mobile Apps - Environment Variables Example
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://xlcsegukheumsadygmgh.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Environment
EXPO_PUBLIC_APP_ENV=development
EXPO_PUBLIC_API_VERSION=v1

# App Configuration
EXPO_PUBLIC_APP_NAME=Harry School
EXPO_PUBLIC_APP_VERSION=1.0.0

# Educational Configuration
EXPO_PUBLIC_ORGANIZATION_ID=harry-school-tashkent
EXPO_PUBLIC_DEFAULT_LANGUAGE=en
EXPO_PUBLIC_SUPPORTED_LANGUAGES=en,ru,uz

# Feature Flags
EXPO_PUBLIC_ENABLE_BIOMETRIC_AUTH=true
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=true
EXPO_PUBLIC_ENABLE_REAL_TIME=true
EXPO_PUBLIC_ENABLE_ANALYTICS=true

# Development Configuration
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=debug
EXPO_PUBLIC_ENABLE_FLIPPER=true

# Performance Configuration
EXPO_PUBLIC_MAX_CACHE_SIZE=50MB
EXPO_PUBLIC_OFFLINE_QUEUE_LIMIT=1000
EXPO_PUBLIC_SYNC_INTERVAL=30000

# Security Configuration
EXPO_PUBLIC_SESSION_TIMEOUT=3600000
EXPO_PUBLIC_MAX_LOGIN_ATTEMPTS=5
EXPO_PUBLIC_ENABLE_DEVICE_BINDING=true

# Educational Features
EXPO_PUBLIC_AI_HOMEWORK_GENERATION=true
EXPO_PUBLIC_VOCABULARY_LEARNING=true
EXPO_PUBLIC_PROGRESS_TRACKING=true
EXPO_PUBLIC_ACHIEVEMENT_SYSTEM=true

# Third-party Services (if needed)
EXPO_PUBLIC_OPENAI_API_KEY=your_openai_key_for_ai_features
EXPO_PUBLIC_ANALYTICS_KEY=your_analytics_key_here
EXPO_PUBLIC_CRASH_REPORTING_KEY=your_crash_reporting_key_here