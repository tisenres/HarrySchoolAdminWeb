{
  "name": "harry-school-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:fast": "NODE_ENV=development FAST_DEV=true next dev",
    "dev:no-lint": "SKIP_LINT=true next dev",
    "dev:no-type": "SKIP_TYPE_CHECK=true next dev",
    "dev:turbo": "NODE_ENV=development FAST_DEV=true SKIP_LINT=true next dev --turbo",
    "dev:prewarm": "concurrently \"npm run dev\" \"sleep 5 && npm run prewarm\"",
    "prewarm": "node scripts/prewarm-routes.js",
    "build": "cross-env SKIP_TYPE_CHECK=true SKIP_LINT=true DISABLE_STATIC_GENERATION=true next build --no-lint",
    "build:fast": "cross-env SKIP_TYPE_CHECK=true SKIP_LINT=true next build",
    "build:analyze-fast": "cross-env ANALYZE=true SKIP_TYPE_CHECK=true next build",
    "start": "next start",
    "preview": "next build && next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "next lint --max-warnings 0",
    "lint:fast": "eslint --config eslint.config.dev.mjs src/",
    "lint:report": "next lint --format compact --output-file eslint-report.txt",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "format:staged": "prettier --write",
    "type-check": "tsc --noEmit",
    "type-check:dev": "tsc --project tsconfig.dev.json --noEmit",
    "type-check:incremental": "tsc --noEmit --incremental --tsBuildInfoFile .next/cache/typecheck.tsbuildinfo",
    "type-check:watch": "tsc --noEmit --watch",
    "type-check:fast": "tsc --project tsconfig.dev.json --noEmit --skipLibCheck",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathIgnorePatterns=e2e",
    "test:integration": "jest --testPathPattern=services",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "test:teachers": "jest --testPathPattern=teachers --coverage --coverageDirectory=coverage/teachers",
    "test:e2e": "puppeteer test",
    "test:e2e:ui": "puppeteer test --ui",
    "test:e2e:headed": "puppeteer test --headed",
    "test:e2e:debug": "puppeteer test --debug",
    "test:e2e:teachers": "puppeteer test teachers/teachers-crud.spec.ts",
    "test:e2e:groups": "puppeteer test groups/groups-crud.spec.ts",
    "test:e2e:students": "puppeteer test students/students-crud.spec.ts",
    "test:e2e:chrome": "puppeteer test --project=chromium",
    "test:e2e:firefox": "puppeteer test --project=firefox",
    "test:e2e:safari": "puppeteer test --project=webkit",
    "test:e2e:mobile": "puppeteer test --project='Mobile Chrome'",
    "test:e2e:report": "puppeteer show-report",
    "test:e2e:trace": "puppeteer show-trace",
    "test:e2e:setup": "ts-node e2e/helpers/test-data-setup.ts",
    "test:e2e:cleanup": "ts-node e2e/helpers/test-data-setup.ts --cleanup",
    "test:e2e:validate": "node scripts/test-e2e-setup.js",
    "test:performance": "jest --testPathPattern=performance",
    "test:performance:benchmark": "node scripts/performance-benchmark.js",
    "test:lighthouse": "lhci autorun",
    "test:lighthouse:ci": "lhci autorun --config=lighthouserc.js",
    "test:comprehensive": "node scripts/run-comprehensive-tests.js",
    "test:critical-errors": "jest --testPathPattern=critical-errors --verbose",
    "test:api-integration": "jest --testPathPattern=api --verbose",
    "test:components-critical": "jest --testPathPattern=components/critical --verbose",
    "test:environment": "jest --testPathPattern=setup/test-environment --verbose",
    "quality": "npm run type-check && npm run lint:strict && npm run format:check",
    "quality:fix": "npm run format && npm run lint:fix && npm run type-check",
    "quality:ci": "npm run type-check && npm run lint:strict && npm run format:check && npm run test:ci",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase db push",
    "db:types": "supabase gen types typescript --local > src/types/database.ts",
    "db:seed": "supabase db reset --db-url $DATABASE_URL",
    "pre-commit": "npm run quality:fix",
    "pre-push": "npm run quality:ci",
    "prepare": "husky install || true",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "analyze:watch": "cross-env ANALYZE=true next dev",
    "analyze:size": "npm run build && npx @next/bundle-analyzer",
    "performance:check": "node scripts/performance-check.js",
    "performance:monitor": "node scripts/build-monitor.js",
    "performance:history": "node scripts/build-monitor.js history",
    "deps:analyze": "node scripts/dependency-optimizer.js",
    "deps:optimize": "node scripts/dependency-optimizer.js check",
    "clean": "rimraf .next out dist coverage *.tsbuildinfo",
    "clean:deps": "rimraf node_modules package-lock.json && npm install",
    "clean:all": "npm run clean && npm run clean:deps",
    "upgrade": "npx npm-check-updates -u && npm install",
    "security": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@playwright/test": "^1.54.2",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.53.0",
    "@tanstack/react-query": "^5.84.1",
    "@types/ioredis": "^4.28.10",
    "@types/jszip": "^3.4.0",
    "@types/lodash": "^4.17.20",
    "@types/react-window": "^1.8.8",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "critters": "^0.0.23",
    "cross-env": "^7.0.3",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.34.0",
    "eslint-config-next": "15.4.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.23.12",
    "ioredis": "^5.7.0",
    "isomorphic-dompurify": "^2.26.0",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.536.0",
    "next": "15.4.5",
    "next-intl": "^4.3.4",
    "react": "18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.62.0",
    "react-is": "^18.3.1",
    "react-window": "^1.8.11",
    "recharts": "^3.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "web-vitals": "^5.1.0",
    "xlsx": "^0.18.5",
    "zod": "^4.0.14",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.12.0",
    "@next/bundle-analyzer": "^15.4.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.19.11",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "axe-core": "^4.8.0",
    "concurrently": "^9.2.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "node-fetch": "^3.3.2",
    "npm-check-updates": "^17.1.3",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "puppeteer": "^23.11.1",
    "rimraf": "^6.0.1",
    "tw-animate-css": "^1.3.7",
    "typescript": "5.9.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run quality:ci",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
